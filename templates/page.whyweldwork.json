    "163819695138a88612": {
      "type": "perks",
      "blocks": {
        "bb453e10-d61c-46ed-ad5f-cf9d088952a4": {
          "type": "Tab",
          "settings": {
            "title": "100% in-house production",
            "titlePerk": "Done by us",
            "titleCon": "Outsourced to third-party",
            "textPerk": "All of our production is done by us, 100% in-house. This means that we can always ensure the highest level of finish with every single project.",
            "textCon": "Many competitors on the market outsource to third-party fabricators. Which means compromising on quality, and unpredictable & inconsistent quality from product to product."
          }
        },
        "4edbe044-5a42-40ef-82cd-0c1c4d20c54b": {
          "type": "Tab",
          "settings": {
            "title": "Guaranteed customer care",
            "titlePerk": "Always speak to a live Weldwork specialist.",
            "titleCon": "Customer service not guaranteed.",
            "textPerk": "We have a 100% U.S. based customer care team who is well trained in everything Weldwork - so we can ensure the highest level of customer service possible.",
            "textCon": "Many of our competitors don’t have a dedicated customer care team, or they outsource to agents who may not fully understand or be able to help with your project."
          }
        },
        "5a48ef25-9f3f-40eb-89b7-52892182026b": {
          "type": "Tab",
          "settings": {
            "title": "Locally sourced materials",
            "titlePerk": "Guaranteed material quality & availability.",
            "titleCon": "Shipping delays, low quality materials.",
            "textPerk": "All of our materials are 100% locally sourced, and built by highly skilled American workers. It means we never skimp on quality, or experience international shipping delays.",
            "textCon": "Many of our competitors import materials, meaning quality can be inconsistent. It is also common to experience unpredictable, shipping delays that can hurt your project timeline."
          }
        }
      },
