<script>
  let srcImg = "";
  $(".indTab").click(function(){
    srcImg = $(this).attr("background");
    $(".backgroundImageHidden").removeClass("active");
    setTimeout(function(){
      $(".imageSetHeight").attr("src", srcImg);
      $(".backgroundImageHidden").on("load", function() {
         $(".backgroundImageHidden").addClass("active"); 
         setTimeout(function(){
         $(".backgroundImageHidden2").attr("src", srcImg);
         },600);
      }).attr("src", srcImg);
    },600);
    $("#shopify-section-{{ section.id }}").css("background-image", "url("+srcImg+")");
  	$(".tabText.active").addClass("unactive");
    $(".tabText.unactive").removeClass("active");
    $(".textTab-"+$(this).attr("tab")).removeClass("unactive");
    $(".textTab-"+$(this).attr("tab")).addClass("active");
    $(".indTab").removeClass("active");
    $(this).addClass("active");
    $(".indTab .numberTab").show();
    $(".indTab .arrowTab").hide();
    $(".indTab.active .numberTab").hide();
    $(".indTab.active .arrowTab").show();
  });
  $(".indTab").hover(function(){
    let thistab = $(this);
      thistab.click();
  });
  $(".indTab").first().click();
  window.addEventListener('load', (event) => {
   if ($(window).width() < 1009) {
  	$("#shopify-section-{{ section.id }}").css("height", $("#shopify-section-{{ section.id }}").css("height"));
   }               
  });
</script>
