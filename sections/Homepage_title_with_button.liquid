            <p class="GT-300 text-{{ section.id }}">{{ section.settings.text }}</p>
          </div>
            <style>
  .text-{{ section.id }} {
                margin: 0; 
                letter-spacing: {{ section.settings.textLetter }}em; 
                font-size: {{ section.settings.textSize | times: 100 | divided_by: 1920.0 }}vw; 
                color: {{ section.settings.textColor }}; 
                line-height: {{ section.settings.textLine | times: 100 | divided_by: 1920.0 }}vw;
                text-align: {{ section.settings.textAlign }};
                margin-bottom: {{ section.settings.textMarBottom | times: 100 | divided_by: 1920.0 }}vw;
               
              }

        @media only screen and (min-width:1440px) and (max-width: 1919px) {
 .text-{{ section.id }} {
                
                font-size: {{ section.settings.textSize | times: 100 | divided_by: 1919.0 }}vw; 
                
                line-height: {{ section.settings.textLine | times: 100 | divided_by: 1919.0 }}vw;
               
                margin-bottom: {{ section.settings.textMarBottom | times: 100 | divided_by: 1919.0 }}vw;
               
