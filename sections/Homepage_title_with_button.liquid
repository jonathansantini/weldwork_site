<style>
 .customcenter {text-align:center;}
  


  @media only screen and (min-width:1009px)  {

   #shopify-section-{{ section.id }} {
    margin-top: {{ section.settings.marTop | times: 100 | divided_by: 1440.0 }}vw;
    margin-bottom: {{ section.settings.marBottom | times: 100 | divided_by: 1440.0 }}vw;
    padding-top: {{ section.settings.padTop | times: 100 | divided_by: 1440.0 }}vw;
    padding-bottom: {{ section.settings.padBottom | times: 100 | divided_by: 1440.0 }}vw;
    position: relative;
    background-color: {{ section.settings.backgroundColor }};
    z-index: 3;
  }
  }


 @media only screen and (max-width:1008px) {

      .customcenter {padding:0px!important;}
    #shopify-section-{{ section.id }} {
      margin-top: {{ section.settings.marTopMob }}px;
      margin-bottom: {{ section.settings.marBottomMob }}px;
      padding-top: {{ section.settings.padTopMob }}px;
      padding-bottom: {{ section.settings.padBottomMob }}px;
     
    }
 }


  

@media only screen and (max-width: 330px){
#shopify-section-165954056797982f71 {
  
    margin: 0 auto;
    margin-bottom: 72px;
      margin-left: 20px;
    margin-right: 20px;
}
  .customcenter h3.heading-165954056797982f71 {
    width: 100%;}
}
</style>
{% for block in section.blocks %}
	{% if block.settings.image != blank %}<img data-bottom-top="transform: translateY(0%)" data-top-bottom="transform: translateY(-50%)" class="image-{{ block.id }}" src="{{ block.settings.image | img_url: '700x' }}">{% endif %}
	<style>
       @media only screen and (min-width: 1009px) {
      .image-{{ block.id }} {
        position: absolute;
        {% if block.settings.top != blank %}top: {{ block.settings.top | times: 100 | divided_by: 1440.0 }}vw;{% endif %}
        {% if block.settings.bottom != blank %}bottom: {{ block.settings.bottom | times: 100 | divided_by: 1440.0 }}vw;{% endif %}
        {% if block.settings.left != blank %}left: {{ block.settings.left | times: 100 | divided_by: 1440.0 }}vw;{% endif %}
        {% if block.settings.right != blank %}right: {{ block.settings.right | times: 100 | divided_by: 1440.0 }}vw;{% endif %}
        z-index: {{ block.settings.zIndex }};
        width: {{ block.settings.width | times: 100 | divided_by: 1440.0 }}vw;
      }
      }
      @media only screen and (max-width: 1009px) {
      	.image-{{ block.id }} {
        position: absolute;
        {% if block.settings.topMob != blank %}top: {{ block.settings.topMob }}px;{% endif %}
        {% if block.settings.bottomMob != blank %}bottom: {{ block.settings.bottomMob }}px;{% endif %}
        {% if block.settings.leftMob != blank %}left: {{ block.settings.leftMob }}px;{% endif %}
        {% if block.settings.rightMob != blank %}right: {{ block.settings.rightMob  }}px;{% endif %}
        z-index: {{ block.settings.zIndex }};
        width: {{ block.settings.widthMob }}px;
      }
     
      }

 
      
	</style>
{% endfor %}
<div class="centeredContainer customcenter applyAnimation" id="test" style="padding: 0 10.2vw;">

  {% if section.settings.title != blank %}
            <h3 class="come-in-1 GT-600 customHeading heading-{{ section.id }}">{{ section.settings.title }}</h3>
            <style>
              h3.heading-{{ section.id }} {
            margin: 0; 
            letter-spacing: {{ section.settings.titleLetter }}em; 
           
            color: {{ section.settings.titleColor }}; 
           
            text-align: {{ section.settings.titleAlign }};
            margin-bottom: {{ section.settings.titleMarBottom | times: 100 | divided_by: 1920.0 }}vw;
          }
        
        @media only screen and (min-width:1009px) {
          h3.heading-{{ section.id }} {
            font-size: {{ section.settings.titleSize | times: 100 | divided_by: 1440.0 }}vw; 
            line-height: {{ section.settings.titleLine | times: 100 | divided_by: 1440.0 }}vw;
            margin-bottom: {{ section.settings.titleMarBottom | times: 100 | divided_by: 1440.0 }}vw;
          }
        }
        
    
        @media only screen and (max-width:1008px) {
         h3.heading-{{ section.id }} {
            margin: 0; 
            color: {{ section.settings.titleColorMob }}; 
            text-align: {{ section.settings.titleAlignMob }};
            margin-bottom: {{ section.settings.titleMarBottomMob }}px;
            font-size: {{ section.settings.titleSizeMob  }}px; 
            letter-spacing: {{ section.settings.titleLetterMob }}em; 
            line-height: {{ section.settings.titleLineMob }}px;
          }
        }
            </style>
        {% endif %}
        
  {% if section.settings.subtext != blank %}
            <p class="come-in-2 applyAnimation subtext-{{ section.id }} GT-300">
              {{ section.settings.subtext }}
      		</p>
            <style>
                .subtext-{{ section.id }} {
                margin: 0; 
