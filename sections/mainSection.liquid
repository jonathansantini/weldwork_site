<style>

  
  .shopify-section {
  	z-index: 2;
    position: relative;
  }
  #shopify-section-mainHeader {
  	z-index: 5;
  }
  .deskMenuItem {
  	z-index: 4;
  }
  #shopify-section-{{ section.id }} .blurredTop {
    background: linear-gradient(180deg, {{ section.settings.backgroundColor }} 0%, rgba(239,239,239,0) 100%); 
  }
  #shopify-section-{{ section.id }} .blurredBottom {
    background: linear-gradient(0deg, {{ section.settings.backgroundColor }}) 0%, rgba(239,239,239,0) 100%);
  }
  #shopify-section-{{ section.id }} {
    margin-top: {{ section.settings.marTop | times: 100 | divided_by: 1440.0 }}vw;
    margin-bottom: {{ section.settings.marBottom | times: 100 | divided_by: 1440.0 }}vw;
    background-color: {{ section.settings.backgroundColor }};
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1;
    transform: translateZ(0);
  }
  @media only screen and (max-width: 1009px) {
    #shopify-section-{{ section.id }} {
      margin-top: {{ section.settings.marTopMob }}px;
      margin-bottom: {{ section.settings.marBottomMob }}px;
      max-width: 100%;
      padding-bottom: 67px;
      position: relative;
      display: block!important;
    }
    #shopify-section-{{ section.id }} .mainSecText {
    	padding: 100px 20px 0px 20px;
    }
    #shopify-section-{{ section.id }} .lottieDiv {
    	position: absolute;
      	top: unset!important;
      	left: unset!important;
      	bottom: 62.3px;
      	right: 60px;
      	z-index: 5;
    }
  }
</style>
<div class="containerMain noCursor">
  <div class="downLottie lottieDiv">
    <lottie-player src="https://cdn.shopify.com/s/files/1/0580/5994/8228/files/data.json?v=1639224308" background="transparent"  speed="1"  style="width: 100px; height: 100px;" loop autoplay></lottie-player>
  </div>
  <div class="divRow">
    <div class="divCol-50 mainSecText {% if template contains 'collection' %}come-in{% endif %}">
      {% if template contains 'index' %}
    	{% if section.settings.subtext != blank %}
            <p class="{% if template contains 'collection' %}come-in-1{% endif %} secondTitleHome subtext-{{ section.id }} GT-400">
              {{ section.settings.subtext }}
      		</p>
            <style>
              .subtext-{{ section.id }} {
                margin: 0; 
                letter-spacing: {{ section.settings.subtextLetter }}px; 
                font-size: {{ section.settings.subtextSize | times: 100 | divided_by: 1440.0 }}vw; 
                color: {{ section.settings.subtextColor }}; 
                line-height: {{ section.settings.subtextLine | times: 100 | divided_by: 1440.0 }}vw;
                text-align: {{ section.settings.subtextAlign }};
                margin-bottom: {{ section.settings.subtextMarBottom | times: 100 | divided_by: 1440.0 }}vw;
              }
              @media only screen and (max-width: 1009px) {
                .subtext-{{ section.id }} {
                  margin: 0; 
                  letter-spacing: {{ section.settings.subtextLetterMob }}px; 
                  font-size: {{ section.settings.subtextSizeMob }}px; 
                  color: {{ section.settings.subtextColorMob }}; 
                  line-height: {{ section.settings.subtextLineMob }}px;
                  text-align: {{ section.settings.subtextAlignMob }};
                  margin-bottom: {{ section.settings.subtextMarBottomMob }}px;
                }
              }
            </style>
        {% endif %}
      	{% if section.settings.title != blank %}
            <h2 class="{% if template contains 'collection' %}come-in-2{% endif %} firstTitleHome CCD customHeading heading-{{ section.id }}">{{ section.settings.title }}</h2>
            <style>
              h2.heading-{{ section.id }} {
                margin: 0; 
                letter-spacing: {{ section.settings.titleLetter | times: 100 | divided_by: 1440.0 }}vw;
                font-size: {{ section.settings.titleSize | times: 100 | divided_by: 1440.0 }}vw; 
                color: {{ section.settings.titleColor }}; 
                line-height: {{ section.settings.titleLine | times: 100 | divided_by: 1440.0 }}vw;
                text-align: {{ section.settings.titleAlign }};
                margin-bottom: {{ section.settings.titleMarBottom | times: 100 | divided_by: 1440.0 }}vw;
              }
              @media only screen and (max-width: 1009px) {
                h2.heading-{{ section.id }} {
                  margin: 0; 
                  letter-spacing: {{ section.settings.titleLetterMob }}px; 
                  font-size: {{ section.settings.titleSizeMob }}px; 
                  color: {{ section.settings.titleColorMob }}; 
                  line-height: {{ section.settings.titleLineMob }}px;
                  text-align: {{ section.settings.titleAlignMob }};
                  margin-bottom: {{ section.settings.titleMarBottomMob }}px;
                }
              }
            </style>
        {% endif %}
      {% else %}
      	{% if section.settings.title != blank %}
            <h2 class="{% if template contains 'collection' %}come-in-1{% endif %} firstTitleHome CCD customHeading heading-{{ section.id }}">{{ section.settings.title }}</h2>
            <style>
              h2.heading-{{ section.id }} {
                margin: 0; 
                letter-spacing: {{ section.settings.titleLetter | times: 100 | divided_by: 1440.0 }}vw;
                font-size: {{ section.settings.titleSize | times: 100 | divided_by: 1440.0 }}vw; 
