<style>
  #shopify-section-{{ section.id }} {
    margin-top: {{ section.settings.marTop | times: 100 | divided_by: 1440.0 }}vw;
    margin-bottom: {{ section.settings.marBottom | times: 100 | divided_by: 1440.0 }}vw;
    background-image: url({{ section.settings.backgroundImage | img_url: "1200x" }});
    position: relative;
    background-size: cover;
  }
  #shopify-section-{{ section.id }} .visibilityDiv {
  	padding-top: {{ section.settings.padTop | times: 100 | divided_by: 1440.0 }}vw;
    padding-bottom: {{ section.settings.padBottom | times: 100 | divided_by: 1440.0 }}vw;
    padding-left: {{ section.settings.padLeft | times: 100 | divided_by: 1440.0 }}vw;
    padding-right: {{ section.settings.padRight | times: 100 | divided_by: 1440.0 }}vw;
  }

#shopify-section-ProductionPageTextonimage .textDiv-ProductionPageTextonimage {
   

    margin-left: 10%;
}
#shopify-section-ProductionPageTextonimage .boxDiv-ProductionPageTextonimage {
    width: 26%;
}



.come-in .come-in-2 {
    transform: translate3d(0,3.86364rem,0) scaleZ(1) rotateX(0) rotateY(0) rotate(0) skew(0);
    animation: come-in .6s ease-out .5s forwards;
    animation-delay: .8s;
    opacity: 0;
}

.subtext-ProductionPageTextonimage {
    font-weight: 200!important;
}
  #shopify-section-ProductionPageTextonimage .boxDiv-ProductionPageTextonimage {

    margin-top: 44px;}

  #shopify-section-ProductionPageTextonimage .boxDiv-ProductionPageTextonimage {
   border-top: 2px solid #FFFFFF;
    }

    #shopify-section-ProductionPageTextonimage .visibilityDiv {
   
    background-color: rgb(11 11 10 / 60%);
}


  @media only screen and (max-width: 1009px) {



  #shopify-section-ProductionPageTextonimage .textDiv-ProductionPageTextonimage {
     margin-left: 0%; 
}

    
 
#shopify-section-ProductionPageTextonimage .boxDiv-ProductionPageTextonimage {
